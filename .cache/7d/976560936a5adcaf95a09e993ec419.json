    super(props)\n    this.state = props.userProfile ? {...props.userProfile} :{\n      state: '',\n      country: '',\n      image: '',\n      birthdate: '',\n    };\n  }\n  componentWillReceiveProps(props) {\n    if (props.userProfile) this.setState(props.userProfile);\n  }\n  handleSubmit = e => {\n    e.preventDefault();\n    return this.props.onComplete(this.state);\n  };\n  handleChange = e => {\n    this.setState({\n      [e.target.name]: e.target.value\n    })\n  };\n  render() {\n    let dateClass = !this.state.birthdate ? 'grayDate' : '';\n    return (\n      <section className='profile-form'>\n        <form className='form' onSubmit={this.handleSubmit}>\n          <h2 className='title'>{this.props.profileAction} your profile.</h2>\n          <label htmlFor='state' className='profileFormLabel'>State: </label>\n          <input \n            type=\"text\" \n            placeholder=\"State\" \n            value={this.state.state || ''}\n            name=\"state\"\n            onChange={this.handleChange}/>\n          <label htmlFor='country' className='profileFormLabel'>Country: </label>\n          <input \n            type=\"text\" \n            placeholder=\"Country\"\n            value={this.state.country || ''}\n            name=\"country\"\n            onChange={this.handleChange}/>\n          <label htmlFor='image' className='profileFormLabel'>Profile img URL: </label>\n          <input \n            type=\"text\" \n            placeholder=\"img url\"\n            value={this.state.image || ''}\n            name=\"image\"\n            onChange={this.handleChange}/>\n          <p className='textRight'><button className='red-button b-button' type=\"submit\">Submit</button></p>\n        </form>  \n      </section>\n    );\n  }    \n}  \n\nexport default ProfileForm;"},"lineCount":null}},"error":null,"hash":"b7e3d45c682ed9e7fd859c1e75f8158a","cacheData":{"env":{}}}