l":{"line":46,"column":6}},{"generated":{"line":127,"column":22},"source":"components/comment-form/index.js","original":{"line":45,"column":4}},{"generated":{"line":128,"column":0},"source":"components/comment-form/index.js","original":{"line":73,"column":3}},{"name":"React","generated":{"line":132,"column":2},"source":"components/comment-form/index.js","original":{"line":5,"column":26}},{"name":"Component","generated":{"line":132,"column":17},"source":"components/comment-form/index.js","original":{"line":5,"column":32}},{"name":"CommentForm","generated":{"line":134,"column":15},"source":"components/comment-form/index.js","original":{"line":76,"column":15}}],"sources":{"components/comment-form/index.js":"import React from 'react';\n\nimport { renderIf } from '../../lib/util';\n\nclass CommentForm extends React.Component {\n  constructor(props) {\n    super(props);\n    this.textAreaHeight = React.createRef();\n    this.state = {\n      content: '',\n      textAreaHeight: 37,\n    };\n  }\n\n  componentWillUnmount() {\n    this.setState({ content: '', textAreaHeight: 37 });\n  }\n\n  handleChange = e => {\n    let currHeight = this.textAreaHeight.current.scrollHeight;\n    if(currHeight > this.state.textAreaHeight) \n      this.setState({ textAreaHeight: currHeight });\n\n    let { name, value } = e.target;\n\n    this.setState({\n      [name]: value,\n    });\n  };\n\n  handleSubmit = e => {\n    e.preventDefault();\n    if(this.state.content) {\n      this.props.onComplete({content: this.state.content })\n        .catch(err => console.error(err));\n    }\n    this.setState({ content: '', textAreaHeight: 37 });\n  };\n\n  render() {\n    let { focused, content} = this.state;\n    let textAreaStyle = {\n      height: `${this.state.textAreaHeight}px`,\n    };\n    return (\n      <form onSubmit={this.handleSubmit} className='form comment-form'>\n        <div className='photo-div'>\n          <img src={this.props.image} />\n        </div>\n        <div className='commentInput-div'>\n          <div className='commentInputWrapper'>\n            <textarea\n              className='commentInput'\n              type='content'\n              name='content'\n              placeholder='add a comment...'\n              value={this.state.content}\n              onChange={this.handleChange}\n              ref={this.textAreaHeight}\n              style={textAreaStyle}\n            ></textarea>\n          </div>\n        </div>\n        {renderIf(content,\n          <div className='commentFormButtonDiv'>\n            <div className='ButtonDiv'>\n              <button className='button' type='submit'> post </button>\n            </div>\n          </div>\n        )}\n      </form>\n    );\n  }\n}\n\nexport default CommentForm;"},"lineCount":null}},"error":null,"hash":"644ad733aabbb197e784f935200ec71e","cacheData":{"env":{}}}